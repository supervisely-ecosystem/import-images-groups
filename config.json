  "icon_cover": false,
  "icon_background": "#FFFFFF",
  "headless": true,
  "min_instance_version": "6.11.8",
  "context_menu": {
    "target": [
      "files_folder",
      "files_file"
    ]
  },
  "poster": "https://github.com/user-attachments/assets/cefddf42-82b0-44b8-843c-820c6196f6cf"
